{
    "name": "giorgijorji/laravel-tbc-installment",
    "description": "TBC installment for laravel",
    "keywords": [
        "giorgijorji",
        "tbc",
        "laravel",
        "installment"
    ],
    "authors": [
        {
            "name": "Z3R0",
            "email": "giorgi.jorji@gmail.com",
            "role": "Developer"
        }
    ],
    "homepage": "https://github.com/giorgijorji/laravel-tbc-installment",
    "license": "MIT",
    "require": {
        "php": "^7.1 || ^8.0",
        "guzzlehttp/guzzle": "^7.2",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "Giorgijorji\\LaravelTbcInstallment\\" : "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Giorgijorji\\LaravelTbcInstallment\\TbcInstallmentServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
